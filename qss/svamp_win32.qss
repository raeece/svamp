/* R10 */
QComboBox * {
    font: 9pt;
}

/* Rule 9 */
QComboBox::drop-down:!editable {
    subcontrol-origin: padding;
    subcontrol-position: center right;
    width: 11px;
    height: 6px;
    background: none;
}

