/* Rule 6 */
/*QComboBox:!editable,*/
QPushButton {
background-color: rgb(110,152,184);
     border-style: outset;
     border-width: 2px;
     border-color: beige;
     padding: 4px;
}

/* Rule 7 */
QComboBox:!editable:hover,
QComboBox::drop-down:editable:hover,
QPushButton:hover {
background-color: rgb(230,252,184);
}

/* Rule 8 */
QComboBox:!editable:on,
QPushButton:pressed {
    color: lightgray;
	background-color: rgb(125,200,100);
    padding-top: -15px;
    padding-bottom: -17px;
}

